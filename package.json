    "sass": "^1.75.0",
    "sass-math": "^1.0.0",
    "sharp": "^0.33.3",
    "shepherd.js": "^11.2.0",
    "shikiji": "^0.10.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "swiper": "^11.1.1",
    "tsx": "^4.7.2",
