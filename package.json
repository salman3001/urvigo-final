  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app",
    "rules": {
      "unicorn/filename-case": "off"
    }
  },
  "prettier": "@adonisjs/prettier-config",
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/**/*.ts",
      "./database/**/*.ts",
      "./resources/*.ts"
    ]
  },
  "lint-staged": {
    "./**/*.js": [
      "eslint ",
      "prettier --write "
    ],
    "./**/*.ts": [
      "eslint ",
      "prettier --write "
    ],
    "./**/*.vue": [
      "eslint ",
      "prettier --write "
    ]
  }
